package chapter02;

public class Ramyun {
	public static void main(String[] args) {
		buy();
		boil();
	}
	
	// void 반환타입
	// boil은 메소드
	// ()안에 들어가는 것은 입력 변수, 매개변수
	public static void boil() {
		System.out.println("물을 끓인다.");
	} 
	// void 반환타입 buy는 메소드 ()안에 들어오는 것은 입력변수, 매개변수
	public static void buy() {
		System.out.println("라면을 사온다.");
	} 
}
