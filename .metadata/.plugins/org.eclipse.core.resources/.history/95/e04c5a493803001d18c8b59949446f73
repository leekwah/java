package chapter02;

import java.util.Scanner;

public class Median {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		System.out.print("정수 3개 입력 >>> ");
		int x = scan.nextInt();
		int y = scan.nextInt();
		int z = scan.nextInt();
		System.out.println(x);
		System.out.println(y);
		System.out.println(z);
		
		
		scan.close();
	}

}
