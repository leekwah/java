package chapter03;

import java.util.Scanner;

public class ArrayMin {
	public static void main(String[] args) {
		int[] arr = new int[7];
		Scanner scan = new Scanner(System.in);
		for (int i = 0; i < arr.length; i++) {
			System.out.print("양수 7개를 입력하세요.");
			scan.nextInt();
		}
		System.out.println(arr);
		scan.close();
	}
	
}
