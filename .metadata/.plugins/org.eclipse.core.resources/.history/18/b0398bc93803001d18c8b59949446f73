package chapter02;

import java.util.Scanner;

public class Median {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		System.out.print("정수 3개 입력 >>> ");
		int x = scan.nextInt();
		int y = scan.nextInt();
		int z = scan.nextInt();
		
		if (x > y && y > z) {
			System.out.println("중간 값은"+y);
		} else if (y > x && x > z){
			System.out.println("중간 값은"+x);
		} else {
			System.out.println("중간 값은"+z);
		}
		
		scan.close();
	}

}
