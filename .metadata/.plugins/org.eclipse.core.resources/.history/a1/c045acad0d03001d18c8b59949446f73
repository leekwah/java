package chapter02;

import java.util.Scanner;

public class Won2Dollar {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		System.out.print("원화를 입력하세요 >> ");
		int x = s.nextInt();
		System.out.println("원화는 "+x+"원 입니다.");
		
	}
