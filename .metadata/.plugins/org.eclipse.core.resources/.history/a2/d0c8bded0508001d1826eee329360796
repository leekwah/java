package chapter03;

public class CoronaSum {
	public static void main(String[] args) {
		int corona [][] = {
							{11},{12},{13},
							{21},{22},{23},
							{31},{32},{33},
							{41},{42},{43},
							{51},{12},{13},
							{11},{12},{13},
							{11},{12},{13},
							{11},{12},{13},
							{11},{12},{13},
						  };
		
		int sum = 0;
		
		for (int month = 0; month < corona.length; month++) {
			for (int city = 0; city < corona.length; city++) {
				sum += corona[month][city];
			}
		}
		
		System.out.println("1년 전체 코로나 환자수는 "+sum+"입니다.");
	}
}
